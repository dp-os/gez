{
    "name": "@fmfe/genesis-core",
    "version": "2.7.8",
    "description": "Vue SSR Microfront",
    "keywords": [
        "vue",
        "ssr",
        "Microfront",
        "module-federation"
    ],
    "main": "./dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        "./bin/*": "./bin/*",
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "unbuild"
    },
    "bin": {
        "genesis-try-ts-node": "./bin/try-ts-node.cjs"
    },
    "author": "Followme",
    "license": "MIT",
    "gitHead": "6a1678904351e5faadc693766042a010d600e8b7",
    "dependencies": {
        "axios": "1.5.0",
        "del": "6.1.1",
        "ejs": "3.1.9",
        "fflate": "0.8.0",
        "serialize-javascript": "6.0.1",
        "vue": "2.7.15",
        "vue-server-renderer": "2.7.15",
        "write": "2.0.0"
    },
    "devDependencies": {
        "@types/ejs": "3.1.2",
        "@types/serialize-javascript": "5.0.2",
        "@types/write": "^2.0.1",
        "unbuild": "2.0.0"
    },
    "homepage": "https://fmfe.github.io/genesis-docs/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fmfe/genesis.git"
    },
    "bugs": {
        "url": "https://github.com/fmfe/genesis/issues"
    },
    "type": "module"
}
